{
  "verbose": true,
  "roots": [
    "src"
  ],
  "cacheDirectory": "./node_modules/.jest-cache",
  "coveragePathIgnorePatterns": [
      "/node_modules/"
  ],
  "coverageThreshold": {
    "global": {
      "branches": 50,
      "functions": 50,
      "lines": 50,
      "statements": 50
    }
  },
  "coverageReporters": ["cobertura", "json", "text", "lcov"]
}
